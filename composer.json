{
  "name": "daslampe/af-contactform",
  "type": "typo3-cms-extension",
  "description" : "Typo3 Form with Reply-To feature",
  "homepage": "https://github.com/DasLampe/typo3.plugins.af_contactform",
  "authors": [
    {
      "name": "Andr√© Flemming",
      "email": "daslampe@lano-crew.org",
      "homepage": "https://daslampe.lano-crew.org",
      "role": "Developer"
    }
  ],
  "license": ["GPL-3.0-or-later"],
  "keywords": ["TYPO3 CMS", "Contact", "Form", "Email", "Reply-To"],
  "support": {
    "issues": "https://github.com/DasLampe/typo3.plugins.af_contactform/issues"
  },
  "require": {
    "typo3/cms-core": "^11.5",
    "php": "^8.0.0"
  },
  "require-dev": {
    "typo3/coding-standards": "^0.6.0"
  },
  "autoload": {
    "psr-4": {
      "DasLampe\\AfContactform\\": "Classes/"
    }
  },
  "config": {
    "vendor-dir": ".Build/vendor",
    "bin-dir": ".Build/bin",
    "allow-plugins": {
      "typo3/cms-composer-installers": true,
      "typo3/class-alias-loader": true
    }
  },
  "extra": {
    "typo3/cms": {
      "cms-package-dir": "{$vendor-dir}/typo3/cms",
      "extension-key": "af_contactform",
      "composer-mode": true,
      "ignore-as-root": true,
      "web-dir": ".Build/Web"
    }
  }
}
